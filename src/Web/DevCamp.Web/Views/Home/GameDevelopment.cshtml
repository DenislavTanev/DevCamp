@model IEnumerable<DevCamp.Web.ViewModels.DropDownModels.CategoriesDropDownViewModel>

<header>
    <nav class="navbar  justify-content-start border-bottom">
        <div class="container">
            <ul class="nav">
                @foreach (var category in Model)
                {
                      <li class="nav-item">
                         <a class="nav-link" asp-controller="Listings" asp-action="Create">@category.Name</a>
                      </li>
                }
            </ul>
        </div>
    </nav>
</header>
<div>Inside</div>
